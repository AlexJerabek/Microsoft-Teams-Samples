        "@azure/msal-node": "^2.9.0",
        "@azure/openai": "^1.0.0-beta.12",
        "@microsoft/microsoft-graph-client": "^3.0.4",
        "@microsoft/teams-ai": "^1.1.0",
        "adaptivecards": "^3.0.4",
        "azure-storage": "^2.10.7",
        "botbuilder": "^4.22.2",
        "botframework-connector": "^4.22.2",
        "dotenv": "^16.4.5",
        "e": "^0.2.33",
        "expres": "^0.0.5",
        "express": "^4.19.2",
        "fs": "^0.0.1-security",
